<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:dither="true"
            android:shape="rectangle"
            android:useLevel="false" android:visible="true">
            <solid android:color="@color/White" />
            <corners android:radius="@dimen/_100sdp" />
            <size android:width="@dimen/_14sdp" android:height="@dimen/_14sdp" />
            <stroke android:width="@dimen/_3sdp" android:color="#0000ffff" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:dither="true"
            android:shape="rectangle"
            android:useLevel="false" android:visible="true">
            <solid android:color="@color/White" />
            <corners android:radius="@dimen/_100sdp" />
            <size android:width="@dimen/_14sdp" android:height="@dimen/_14sdp" />
            <stroke android:width="@dimen/_3sdp" android:color="@color/White" />
        </shape>
    </item>
</selector>